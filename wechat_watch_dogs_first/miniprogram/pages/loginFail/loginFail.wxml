<view class="page" data-weui-theme="{{theme}}">
	<view class="weui-msg">
		<view class="weui-msg__icon-area"><i class="weui-icon-warn weui-icon_msg"></i></view>
		<view class="weui-msg__text-area">
			<h2 class="weui-msg__title">操作失败</h2>
		</view>
		<view class="weui-msg__tips-area">
			<view class="weui-msg__tips">账号或密码错误</view>
		</view>
		<view class="weui-msg__opr-area">
			<view class="weui-btn-area">
				<a href="javascript:history.back();" class="weui-btn weui-btn_default">退出程序</a>
			</view>
		</view>
		<view class="weui-msg__extra-area">
			<view class="weui-footer">
				<view class="weui-footer__links">
					<a class="weui-footer__link">修改密码</a>
				</view>
				<view class="weui-footer__text">watch dogs 美丽的监控世界</view>
			</view>
		</view>
	</view>
</view>
